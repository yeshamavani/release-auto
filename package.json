{
  "name": "release-auto",
  "private": false,
  "scripts": {},
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "cz-format-extension": "^1.4.0",
    "husky": "^4.2.5",
    "lerna": "^3.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-format-extension"
    }
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}
